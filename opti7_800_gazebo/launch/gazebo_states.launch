<launch>
    <rosparam file="$(find opti7_800_gazebo)/config/opti7_800_gazebo_joint_states.yaml" command="load"/>

    <node name="joint_controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
          output="screen" ns="/opti7_800" args="joint_state_controller" />

    <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher"
        respawn="false" output="screen">
        <remap from="/joint_states" to="/opti7_800/joint_states" />
    </node>

</launch>


